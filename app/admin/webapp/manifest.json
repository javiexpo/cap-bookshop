					"autoExpandSelect" : true,
					"operationMode" : "Server",
					"synchronizationMode" : "None",
					"updateGroupId" : "UpdateGroup"
				}
			}
		},
		"rootView": {
			"viewName": "sap.m.sample.TableExport.Table",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.ui.layout": {}
			}
		},
		"config": {
			"sample": {
				"files": [
					"Table.view.xml",
					"Table.controller.js",
					"Formatter.js",
					"manifest.json"
				]
			}
		}
	}
}
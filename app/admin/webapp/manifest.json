{
	"sap.app": {
		"id": "sap.m.sample.TableExport",
		"applicationVersion": {
			"version": "1.78.0"
		},
		"dataSources" : {
			"admin" : {
				"uri" : "/admin/",
				"type" : "OData",
				"settings" : {
					"odataVersion" : "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"models" : {
			"" : {
				"dataSource" : "admin",
				"preload" : true,
